{
    "compilerOptions": {
      "module": "esnext",
      "target": "esnext",
      "jsx": "preserve",
      "allowJs": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "strict": true,
      "moduleResolution": "node"
    },
    "include": [
      "resources/js/**/*.vue",
      "resources/js/**/*.ts",
      "resources/js/**/*.tsx"
    ],
    "exclude": [
      "node_modules"
    ]
  }


